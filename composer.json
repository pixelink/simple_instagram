{
    "name": "pixelink/simple-instagram",
    "type": "typo3-cms-extension",
    "description": "TYPO3 CMS Extension wich shows an Instagram feed. Instagram access token needed",
    "authors": [
        {
            "name": "Benjamin Riezler",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^8.7.1|| ^9.5.0",
        "pgrimaud/instagram-user-feed": "^6.2",
        "php": ">=7.2.0"
    },
    "autoload": {
        "psr-4": {
            "Pixelink\\SimpleInstagram\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pixelink\\SimpleInstagram\\Tests\\": "Tests"
        }
    },
    "replace": {
        "simple_instagram": "self.version",
        "typo3-ter/simple-instagram": "self.version"
    }
}
